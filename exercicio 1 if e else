#include <iostream>
using namespace std;
int main(){
    int a, b;
    
    cout<<"digite o primeiro numero \n";
    cin>> a;
    cout<<"digite o segundo numero \n";
    cin>> b;
    
    if ( a>b )
    {
    
        cout<<a<<" e o maior numero. \n";
        
     }
     else
     {
     
        cout<<b<<" e o maior numero. \n";
        
     system("pause");
     }
